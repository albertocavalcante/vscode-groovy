{
  "reporter": ["text", "lcov", "html"],
  "include": [
    "client/out/src/**/*.js"
  ],
  "exclude": [
    "client/out/test/**/*.js",
    "client/out/src/**/*.test.js",
    "node_modules/**",
    "**/*.d.ts"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}